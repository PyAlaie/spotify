<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="343.0" prefWidth="555.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.ap.spotify.client.controllers.MusicEdit">
   <children>
      <TextField fx:id="titleTxt" layoutX="51.0" layoutY="28.0" prefHeight="38.0" prefWidth="452.0" promptText="title">
         <font>
            <Font size="19.0" />
         </font>
      </TextField>
      <ComboBox fx:id="albumMenu" layoutX="51.0" layoutY="78.0" prefHeight="25.0" prefWidth="188.0" promptText="Album" />
      <ComboBox fx:id="genreMenu" layoutX="50.0" layoutY="117.0" prefHeight="27.0" prefWidth="189.0" promptText="Genre" />
      <ImageView fitHeight="210.0" fitWidth="210.0" layoutX="294.0" layoutY="75.0" pickOnBounds="true" preserveRatio="true" />
      <Button layoutX="49.0" layoutY="217.0" mnemonicParsing="false" onAction="#save" prefHeight="100.0" prefWidth="192.0" style="-fx-background-color: #1db954;" text="Save" textFill="WHITE">
         <font>
            <Font size="26.0" />
         </font>
      </Button>
      <TextField fx:id="pathTxt" editable="false" layoutX="294.0" layoutY="296.0" prefWidth="210.0" promptText="path" />
      <Button layoutX="49.0" layoutY="155.0" mnemonicParsing="false" onAction="#uploadCoverPic" prefHeight="28.0" prefWidth="193.0" style="-fx-background-color: #11a0fb;" text="Upload Cover" textFill="WHITE">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
